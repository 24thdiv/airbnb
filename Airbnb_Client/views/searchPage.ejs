<main id="site-content" role="main" ng-controller="search-page">
    <div class="map-search">
        <div class="sidebar">
            <input type="hidden" id="location" value="<%= location %>">
            <input type="hidden" id="lat" value="<%= lat %>">
            <input type="hidden" id="long" value="<%= long %>">
            <input type="hidden" id="viewport" value="<%= viewport %>" ng-model="viewport">

            <div class="filters collapse" style="bottom: 606px;">

                <div class="panel-header filters-section show-sm">
                    Filters
                </div>
                <div class="filters-section intro-filter panel-body panel-light">
                    <div class="row">
                        <div class="col-lg-2 col-md-12 text-center-sm text-center-md row-space-sm-1 filter-label">
                            <label>Dates</label>
                        </div>

                        <form class="col-lg-9 trip-form">
                            <div class="row row-condensed">
                                <div class="col-md-4 col-sm-6 row-space-1-sm">
                                    <label for="map-search-checkin" class="screen-reader-only">
                                        Check In
                                    </label>
                                    <input ng-model="checkin" autocomplete="off" id="checkin" type="text"
                                           class="checkin tour-target ui-datepicker-target" placeholder="Check In"
                                           ng-init="checkin = ''">
                                </div>

                                <div class="col-md-4 col-sm-6 row-space-1-sm">
                                    <label for="map-search-checkout" class="screen-reader-only">
                                        Check Out
                                    </label>
                                    <input ng-model="checkout" autocomplete="off" id="checkout" type="text"
                                           class="checkout tour-target ui-datepicker-target" placeholder="Check Out"
                                           ng-init="checkout = ''">
                                </div>

                                <div class="col-md-4 col-sm-12">
                                    <div class="select select-block">
                                        <label for="guest-select" class="screen-reader-only">
                                            Number of guests
                                        </label>
                                        <select ng-model="guests" class="guest-select" id="guest-select" data-prefill=""
                                                ng-init="guests = ''">
                                            <option value="" disabled>Guest</option>
                                            <option value="1"> 1 Guest</option>
                                            <option value="2"> 2 Guests</option>
                                            <option value="3"> 3 Guests</option>
                                            <option value="4"> 4 Guests</option>
                                            <option value="5"> 5 Guests</option>
                                            <option value="6"> 6 Guests</option>
                                            <option value="7"> 7 Guests</option>
                                            <option value="8"> 8 Guests</option>
                                            <option value="9"> 9 Guests</option>
                                            <option value="10"> 10 Guests</option>
                                            <option value="11"> 11 Guests</option>
                                            <option value="12"> 12 Guests</option>
                                            <option value="13"> 13 Guests</option>
                                            <option value="14"> 14 Guests</option>
                                            <option value="15"> 15 Guests</option>
                                            <option value="16"> 16+ Guests</option>
                                        </select>
                                    </div>
                                </div>
                            </div>

                            <div class="simple-dates-message-container hide">
                                <div class="text-kazan space-top-2">
                                    <strong>
                                        <i class="icon icon-currency-inr"></i>
                                        Enter dates for accurate prices and availability.
                                    </strong>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>

                <div class="filters-section room-type-group intro-filter panel-body panel-light">
                    <div class="row">
                        <div class="col-lg-2 col-md-12 text-center-sm text-center-md filter-label">
                            <label>
                                Room Type
                                <span class="icon icon-question" id="room-type-tooltip"></span>
                                <div class="tooltip-room tooltip-left-middle" role="tooltip"
                                     data-trigger="#room-type-tooltip" data-sticky="true" aria-hidden="true"
                                     style="left: 120px; top: 60.925px;">
                                    <dl class="panel-body">
                                        <dt>Entire Home</dt>
                                        <dd>Listings where you have the whole place to yourself.</dd>
                                        <dt>Private Room</dt>
                                        <dd>Listings where you have your own room but share some common spaces.</dd>
                                        <dt>Shared Room</dt>
                                        <dd>Listings where you'll share your room or your room may be a common space.
                                        </dd>
                                    </dl>
                                </div>
                            </label>


                        </div>

                        <div class="col-lg-9">
                            <div class="row row-condensed" id="room-options">
                                <div class="col-middle-alt col-sm-4">
                                    <label class="checkbox panel panel-dark">
                                        <input class="pull-right room-type" type="checkbox" ng-model="room_type_1"
                                               value="1" ng-checked="" ng-init="room_type_1 = '1'">
                                        <i class="icon icon-entire-place h5 needsclick"></i>
                                        Entire home/apt
                                    </label>
                                </div>
                                <div class="col-middle-alt col-sm-4">
                                    <label class="checkbox panel panel-dark">
                                        <input class="pull-right room-type" type="checkbox" ng-model="room_type_2"
                                               value="2" ng-checked="" ng-init="room_type_2 = '2'">
                                        <i class="icon icon-private-room h5 needsclick"></i>
                                        Private room
                                    </label>
                                </div>
                                <div class="col-middle-alt col-sm-4">
                                    <label class="checkbox panel panel-dark">
                                        <input class="pull-right room-type" type="checkbox" ng-model="room_type_3"
                                               value="3" ng-checked="" ng-init="room_type_3 = '3'">
                                        <i class="icon icon-shared-room h5 needsclick"></i>
                                        Shared room
                                    </label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="filters-section room-type-group intro-filter panel-body panel-light">
                    <div class="row row-condensed space-sm-7">
                        <div class="col-sm-6">
                            <button class="btn btn-block cancel-btn" id="cancel-filter">Cancel</button>
                        </div>
                        <div class="col-sm-6">
                            <button class="btn btn-block btn-primary apply-filters-btn" id="more_filter_submit" ng-click="apply_filter()">
                                Apply Filters
                            </button>
                        </div>
                    </div>
                </div>

            </div>

            <div class="search-results toggle-hide">
                <div class="outer-listings-container row-space-2">

                    <div class="listings-container">
                        <div class="row">

                            <div class="col-sm-12 row-space-2 col-md-6" ng-repeat="rooms in room_result.data" ng-cloak>
                                <div class="listing list_view" ng-mouseover="on_mouse($index);"
                                     ng-mouseleave="out_mouse($index);" data-room_id="{{rooms.id}}">

                                    <div class="panel-image listing-img">
                                        <div class="listing-description wl-data-{{ rooms.id }}">
                                            <div class="summary">
                                                <p>
                                                    {{ rooms.summary }}
                                                    <a href="/property/?propertyId={{rooms.id}}&checkin={{ checkin }}&checkout={{ checkout }}&guests={{ guests }}&room_type={{room_type}}"
                                                       id="tooltip-sticky-{{ rooms.id }}" class="learn-more">Learn
                                                        More</a>
                                                </p>

                                            </div>
                                            <p class="address">{{ rooms.city }}</p>

                                        </div>
                                        <a href="/property?propertyId={{rooms.id}}&checkin={{ checkin }}&checkout={{ checkout }}&guests={{ guests }}&room_type={{room_type}}" target="listing_{{ rooms.id }}"
                                           class="media-photo media-cover">
                                            <div class="listing-img-container media-cover text-center">
                                                <img id="rooms_image_{{ rooms.id}}"  src="http://localhost:3000/images/{{ rooms.photo_name }}" class="img-responsive-height" alt="{{ rooms.name }}">
                                            </div>
                                        </a>

                                        <div class="target-prev target-control rooms-slider block-link" data-room_images="{{rooms.images}}" data-room_id="{{rooms.id}}">
                                            <i class="icon icon-chevron-left icon-size-2 icon-white"></i>
                                        </div>

                                        <a href="/property?propertyId={{rooms.id}}&checkin={{ checkin }}&checkout={{ checkout }}&guests={{ guests }}&room_type={{room_type}}" target="listing_{{ rooms.id }}"
                                           class="link-reset panel-overlay-bottom-left panel-overlay-label panel-overlay-listing-label">
                                            <div>
                                                <sup class="h6 text-contrast"><span
                                                            ng-bind-html="rooms.rooms_price.currency.symbol"></span></sup>
                                                <span class="h3 text-contrast price-amount"> {{ rooms.rooms_price.night }}</span>
                                                <sup class="h6 text-contrast"></sup>
                                                <span aria-label="Book Instantly" data-behavior="tooltip"
                                                      class="h3 icon-beach"
                                                      ng-if="rooms.booking_type == 'instant_book'">
                      <i class="icon icon-instant-book icon-flush-sides"></i>
                  </span>
                                            </div>

                                        </a>
                                        <div class="target-next target-control rooms-slider block-link"  data-room_images="{{rooms.images}}" data-room_id="{{rooms.id}}">
                                            <i class="icon icon-chevron-right icon-size-2 icon-white"></i>
                                        </div>

                                        <div class="panel-overlay-top-right wl-social-connection-panel">
                  <span class="rich-toggle wish_list_button wishlist-button not_saved">
                    <input type="checkbox" id="wishlist-widget-{{ rooms.id }}" name="wishlist-widget-{{ rooms.id }}"
                           data-for-hosting="{{ rooms.id }}" ng-checked="rooms.saved_wishlists">
                    <label for="wishlist-widget-{{ rooms.id }}" class="hide-sm">
                      <span class="screen-reader-only">Wishlist</span>
                      <i class="icon icon-heart icon-rausch icon-size-2 rich-toggle-checked"></i>
                      <i class="icon icon-heart wishlist-heart-unchecked icon-size-2 rich-toggle-unchecked"></i>
                      <i class="icon icon-heart-alt icon-white icon-size-2" id="wishlist-widget-icon-{{ rooms.id }}"></i>
                    </label>
                  </span>

                                        </div>

                                    </div>

                                    <div class="panel-body panel-card-section">
                                        <div class="media">
                                            <a href="/profile?hostId={{rooms.id}}"
                                               class="media-photo-badge pull-right card-profile-picture card-profile-picture-offset">
                                                <div class="media-photo media-round">
                                                    <img src="{{ rooms.users.profile_picture.src }}" alt="">
                                                </div>
                                            </a>
                                            <a href="/property?propertyId={{rooms.id}}&checkin={{ checkin }}&checkout={{ checkout }}&guests={{ guests }}&room_type={{room_type}}" target="listing_{{ rooms.id }}"
                                               class="text-normal">
                                                <h3 title="{{ rooms.name }}" itemprop="name"
                                                    class="h5 listing-name text-truncate row-space-top-1">
                                                    {{ rooms.name }}
                                                </h3>
                                            </a>
                                            <div itemprop="description"
                                                 class="text-muted listing-location text-truncate"><a
                                                        href="/property?propertyId={{rooms.id}}&checkin={{ checkin }}&checkout={{ checkout }}&guests={{ guests }}&room_type={{room_type}}"
                                                        class="text-normal link-reset">
                                                    {{ rooms.room_type_name }}
                                                    <span ng-show="rooms.overall_star_rating">
  ·
  <span ng-bind-html="rooms.overall_star_rating"></span>
  </span>
                                                    <span ng-show="rooms.reviews_count">
  ·
  {{ rooms.reviews_count }} Review{{ (rooms.reviews_count > 1) ? 's' : '' }}
  </span>
                                                </a>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>

                            <h2 ng-hide="room_result.data.length" class="text-center" id="no_results" ng-cloak>
                                No Results Found
                            </h2>

                        </div>

                    </div>

                </div>

